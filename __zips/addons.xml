<addons>
<addon id="plugin.video.crewsade" name="CrEwSaDe" version="1.8.06" provider-name="Crazycook">
    <requires>
        <import addon="xbmc.python" version="2.19.0" />
        <import addon="script.module.universalscrapers" version="1.0.0" />
        <import addon="script.module.python.koding.aio" version="0.9.6.6" />
        <import addon="script.module.beautifulsoup" version="3.2.1" />
        <import addon="script.module.requests" version="2.9.1" />
        <import addon="script.module.addon.common" version="2.0.0" />
        <import addon="script.module.xmltodict" version="0.9.0" />
        <import addon="script.module.dateutil" />
        <!--<import addon="script.module.web-pdb" version="1.0"/>-->
        <import addon="script.module.resolveurl" />
        <!--<import addon="script.module.urlresolver" />-->
    </requires>
    <extension point="xbmc.python.pluginsource" library="default.py">
        <provides>audio video</provides>
    </extension>
    <extension point="xbmc.service" library="service.py" start="[login|startup]" />
    <extension point="xbmc.addon.metadata">
        <summary lang="en">CrEwSaDe</summary>
        <description lang="en">This is a community maintained template for the Great People at RSC.</description>
